<!-- MMAPI PROPERTIES required by the specification -->
<configuration>
<properties>
<!-- SYSTEM PROPERTIES -->
<!-- Visible from System.getProperty() -->
    <!-- JSR135(MMAPI) PROPERTIES -->
    <property Key="microedition.media.version"
      Value="1.1"
      Scope="system"
      Comment="Version number."/>

    <property Key="supports.mixing"
      Value="true"
      Scope="system"
      Comment="Mixing support."/>

    <property Key="supports.audio.capture"
      Value="true"
      Scope="system"
      Comment="Audio capture support."/>

    <property Key="supports.video.capture"
      Value="false"
      Scope="system"
      Comment="Video capture support."/>

    <property Key="supports.recording"
      Value="true"
      Scope="system"
      Comment="Recording support."/>

    <property Key="audio.encodings" 
      Value="encoding=pcm" 
	  Scope="system"/>

